/boot                mtd          boot                                            flags=backup=0
/recovery            mtd          recovery                                        flags=backup=0
/cache               ext4         /dev/block/mtd/by-name/cache
/system              ext4         /dev/block/mtd/by-name/system
/data                ext4         /dev/block/mtd/by-name/userdata                 length=-16384
/external_sd         vfat         /dev/block/mmcblk0p1                            flags=display="EXTERNAL-SD";storage;wipeingui;removable
/usb-otg             vfat         /dev/block/sda1                                 flags=display="USB-OTG";storage;wipeingui;removable
